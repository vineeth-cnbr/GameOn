<% include nav.ejs%>
      <div class="container">
        <div class="row">
                <div class=card>
                <div class="card-content">
                    <span class="card-title white-text">
                    My Profile
                    </span>                    
                </div>
                <div class="card-content">
                   <form >
                    <div class="input-field col s6">
                        <input value="<%=user.username%>" id="username" type="text" class="validate">
                        <label for="username">Username/Email Id</label>
                    </div>
                    <div class="input-field col s6">
                        <input value="<%=user.password%>" id="password" type="password" class="validate">
                        <label for="password">Password</label>
                    </div>
                    <div class="input-field col s6">
                        <input value="<%=user.email%>" id="email" type="email" class="validate">
                        <label for="email">Email</label>
                    </div>
                </form>
                </div>
            
                </div>
        </div>
      </div>
<% include foot.ejs%>